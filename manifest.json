{
  "manifest_version": 2,
 
  "name": "SPP Extender",
  "version": "0.6.1",
  "description": "Provides additional functionality for the SimplePosPool website",

  "icons": { "16": "calc_16.png",
    "48": "calc_48.png",
    "128": "calc_128.png" },

  "permissions": [
    "https://www.coinexchange.io/*",
    "https://coinlib.io/*"
  ],
 
  "content_scripts": [{
    "css": ["content.css", "DataTables/datatables.min.css"],
    "js": ["jquery-3.3.1.min.js", "big.min.js", "loader.js", "moment.js", "content.js", "DataTables/datatables.min.js"],
    "matches": ["https://simplepospool.com/*"],
    "run_at": "document_end"
  }]
 
}