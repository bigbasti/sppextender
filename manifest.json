{
  "manifest_version": 2,
 
  "name": "SPP Extender",
  "version": "0.5.4",
  "description": "Provides additional functionality for the SimplePosPool website",

  "icons": { "16": "calc_16.png",
    "48": "calc_48.png",
    "128": "calc_128.png" },

  "permissions": [
    "https://www.coinexchange.io/*",
    "https://coinlib.io/*"
  ],
 
  "content_scripts": [{
    "css": ["content.css"],
    "js": ["jquery-3.3.1.min.js", "big.min.js", "content.js"],
    "matches": ["https://simplepospool.com/*"],
    "run_at": "document_end"
  }]
 
}